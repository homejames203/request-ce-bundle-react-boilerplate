{
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "no-confusing-arrow": 0,
    "no-else-return": 0,
    "arrow-parens": [2, "as-needed"],
    "react/prop-types": 0,
    "react/jsx-filename-extension": 0,
    "import/prefer-default-export": 0
  },
  "plugins": [
    "react"
  ]
}
